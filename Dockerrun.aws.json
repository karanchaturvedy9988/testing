{
  "AWSEBDockerrunVersion": 2,
   "ContainerDefinitions": [
    {
       "name": "client",
       "image": "7015214645/test-client",
       "hostname": "client",
       "essential": false,
       "memory": 128
    },
    {
      "name": "nginx",
      "image": "7015214645/test-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings":[
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "links": ["client","api"],
      "memory": 128
    },
    {
      "name": "api",
      "image": "7015214645/test-server",
      "hostname": "api",
      "essential":false,
      "memory": 128
    },
    {
      "name": "worker",
      "image":"7015214645/test-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    }
  ]


}
